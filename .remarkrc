{
    "plugins": {
        "lint": {
            "maximum-line-length": 120,
            "fenced-code-flag": false,
            "no-literal-urls": false,
            "list-item-indent": "space",
            "list-item-spacing": false,
            "no-duplicate-headings": false
        }
    },
    "settings": {
        "commonmark": true,
        "bullet": "*",
        "listItemIndent": 1
    }
}
